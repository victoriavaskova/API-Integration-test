{
  "name": "betting-integration-server",
  "version": "1.0.0",
  "type": "module",
  "main": "dist/app.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "dev": "tsx --watch --env-file .env src/app.ts",
    "start": "node --env-file .env dist/app.js",
    "build": "tsc",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage"
  },
  "keywords": [
    "express",
    "typescript",
    "betting",
    "api"
  ],
  "author": "",
  "license": "MIT",
  "description": "Betting integration service with Express and TypeScript",
  "dependencies": {
    "@prisma/client": "^6.11.1",
    "cors": "^2.8.5",
    "dotenv": "^17.0.1",
    "express": "^4.21.2",
    "helmet": "^8.1.0",
    "morgan": "^1.10.0",
    "prisma": "^6.11.1"
  },
  "devDependencies": {
    "@tsconfig/node22": "^22.0.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.10",
    "@types/node": "^24.0.10",
    "bcryptjs": "^3.0.2",
    "jsonwebtoken": "^9.0.2",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4"
  }
}
